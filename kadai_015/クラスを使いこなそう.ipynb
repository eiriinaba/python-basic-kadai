        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"{self.name}\")\n",
        "    print(f\"{self.age}歳\")\n",
        "\n",
        "human = Human(\"サムライタロウ\", 55)\n",
        "human.printinfo()\n",
        "\n"
      ],
